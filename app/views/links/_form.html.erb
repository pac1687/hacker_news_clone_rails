<%= render("layouts/errors", :object => @link) %>

<%= form_for(@link) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :html %>
    <%= f.text_field :html %>
  </div>
  <%= f.submit(:class => "btn btn-primary") %>
<% end %>
